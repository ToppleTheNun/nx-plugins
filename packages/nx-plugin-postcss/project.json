{
  "root": "packages/nx-plugin-postcss",
  "sourceRoot": "packages/nx-plugin-postcss/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/nx-plugin-postcss/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/nx-plugin-postcss"],
      "options": {
        "jestConfig": "packages/nx-plugin-postcss/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nx-plugin-postcss",
        "tsConfig": "packages/nx-plugin-postcss/tsconfig.lib.json",
        "packageJson": "packages/nx-plugin-postcss/package.json",
        "main": "packages/nx-plugin-postcss/src/index.ts",
        "assets": [
          "packages/nx-plugin-postcss/*.md",
          {
            "input": "./packages/nx-plugin-postcss/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/nx-plugin-postcss/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/nx-plugin-postcss",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/nx-plugin-postcss",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    }
  },
  "tags": []
}
